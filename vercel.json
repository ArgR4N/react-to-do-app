{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
{
    "routes": [
      { "src": "/api/(.*)", "dest": "http://localhost:4000/api/$1" }
    ]
}
  